# Пример переменных окружения для Store_Project
# Скопируйте в .env и отредактируйте под свои нужды

# Приложение
APP_NAME=Store_Project
APP_ENV=development
APP_DEBUG=true
APP_PORT=8081

# База данных
DB_HOST=db
DB_PORT=3306
DB_DATABASE=store_db
DB_USERNAME=store_user
DB_PASSWORD=store_password
DB_ROOT_PASSWORD=root_password

# PHP
PHP_MEMORY_LIMIT=256M
PHP_MAX_EXECUTION_TIME=300
PHP_UPLOAD_MAX_FILESIZE=100M
PHP_POST_MAX_SIZE=100M

# Nginx
NGINX_PORT=80
NGINX_HOST=localhost

